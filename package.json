    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/solid-js": "^4.4.2",
    "@astrojs/tailwind": "^5.1.2",
    "astro": "^4.16.18",
    "solid-js": "^1.9.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "wrangler": "^3.94.0"
  }
}
